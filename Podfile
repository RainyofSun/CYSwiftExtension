platform :ios, '13.0'

source 'https://github.com/CocoaPods/Specs.git'

use_frameworks!

# 定义公共库
def CommonPods
   pod 'MJRefresh', '3.7.5'
   pod 'YYKit', '1.0.9'
   pod 'AFNetworking', :git => 'https://github.com/crasowas/AFNetworking.git'
   pod 'Reachability', '3.2'
   pod 'SnapKit', '5.6.0'
   pod 'Toast', '4.1.1'
   pod 'CocoaLumberjack/Swift', '3.8.5'
   pod 'TZImagePickerController', '3.8.8'
   pod 'JKSwiftExtension', "2.7.1"
end

target 'CYSwiftExtension' do
  CommonPods()
end
